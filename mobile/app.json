{
  "expo": {
    "name": "type",
    "slug": "type",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.higorsj.type"
    },
    "web": {
      "favicon": "./src/assets/icon.png"
    },
    "description": "App message, with: Message Queue, Save BD Queue, See online / offline / last message, sign in, sign out, add account.",
    "githubUrl": "https://github.com/higorst/Type_Message_App.git"
  }
}
